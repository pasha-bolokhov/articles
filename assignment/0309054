%%BeginDocument: hp.eps

/tgifdict 7 dict def
tgifdict begin

/tgifellipsedict 6 dict def
tgifellipsedict /mtrx matrix put

/tgifellipse
 { tgifellipsedict begin
      /yrad exch def
      /xrad exch def
      /y exch def
      /x exch def
      /savematrix mtrx currentmatrix def
      x y translate
      xrad yrad scale
      0 0 1 0 360 arc
      savematrix setmatrix
   end
 } def

/tgifarcdict 8 dict def
tgifarcdict /mtrx matrix put

/tgifarcn
 { tgifarcdict begin
      /endangle exch def
      /startangle exch def
      /yrad exch def
      /xrad exch def
      /y exch def
      /x exch def
      /savematrix mtrx currentmatrix def
      x y translate
      xrad yrad scale
      0 0 1 startangle endangle arc
      savematrix setmatrix
   end
 } def

/tgifarc
 { tgifarcdict begin
      /endangle exch def
      /startangle exch def
      /yrad exch def
      /xrad exch def
      /y exch def
      /x exch def
      /savematrix mtrx currentmatrix def
      x y translate
      xrad yrad scale
      0 0 1 startangle endangle arcn
      savematrix setmatrix
   end
 } def

end

tgifdict begin
/tgifsavedpage save def

1 setmiterlimit
1 setlinewidth

0 setgray

72 0 mul 72 3.00 mul translate
72 128 div 100.000 mul 100 div dup neg scale

gsave

/tgiforigctm matrix currentmatrix def

% POLY/OPEN-SPLINE
0 setgray
gsave
   newpath
      224 128 moveto
      448 128 lineto
   tgiforigctm setmatrix
   1 setlinewidth
   stroke
grestore

% POLY/OPEN-SPLINE
0 setgray
gsave
   newpath
      226 63 moveto
      226 64 lineto
   tgiforigctm setmatrix
   1 setlinewidth
   stroke
grestore

% OVAL
0 setgray
gsave
   gsave
      newpath 336 176 48 16 tgifellipse
      stroke
   grestore
grestore

% ARC
0 setgray
gsave
   gsave
      newpath
         336 128 80 60 180 126 tgifarc
      [4 4] 0 setdash
      stroke
   grestore
grestore

% ARC
0 setgray
gsave
   gsave
      newpath
         336 128 80 60 0 53 tgifarcn
      [4 4] 0 setdash
      stroke
   grestore
grestore

% POLY/OPEN-SPLINE
0 setgray
gsave
   [4 4] 0 setdash
   newpath
      336 160 moveto
      336 192 lineto
   tgiforigctm setmatrix
   1 setlinewidth
   stroke
   [] 0 setdash
grestore

grestore
tgifsavedpage restore
end
